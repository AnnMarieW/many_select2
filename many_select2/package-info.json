{
  "name": "many_select2",
  "version": "0.0.1",
  "description": "dmc extension",
  "main": "index.ts",

  "scripts": {
    "build:js::dev": "webpack --mode development",
    "build:js": "webpack",
    "build:backends": "dash-generate-components ./src/ts/components many_select2 -p package-info.json  --ignore \\.test\\.",
    "build": "npm run build:js && npm run build:backends",
    "watch": "npm run build:js::dev -- --watch",
    "dist": "python -m build --sdist --wheel --outdir dist/",
    "format":  "prettier --write src --ignore-path=.prettierignore"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "css-loader": "^6.10.0",
    "npm-run-all": "^4.1.5",
    "react": "18.2.0",
    "react-docgen": "^5.4.3",
    "react-dom": "18.2.0",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.3.1",
    "typescript": "^5.6.3",
    "webpack": "^5.101.0",
    "webpack-cli": "^5.1.4",
    "prettier": "^3.2.4"
  },
  "peerDependencies": {
    "react": "16 - 19",
    "react-dom": "16- 19",
     "@mantine/core": "8.2.7",
    "@mantine/hooks": "8.2.7"
  },
